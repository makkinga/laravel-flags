{
    "name": "makkinga/laravel-flags",
    "description": "A helper for placing SVG flags in your Laravel project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gydo Makkinga",
            "email": "gydo@makkinga.online"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/routing": "^6|^7|^8",
        "illuminate/session": "^6|^7|^8",
        "illuminate/support": "^6|^7|^8"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Makkinga\\Flags\\": "src/"
        },
        "files": [
            "src/Helpers/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makkinga\\Flags\\ServiceProvider"
            ],
            "aliases": {
                "Flag": "Makkinga\\Flags\\Facade"
            }
        }
    }
}
